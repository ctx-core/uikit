{
	"name": "@ctx-core/uikit",
	"version": "4.0.61",
	"description": "ctx-core uikit",
	"keywords": [
		"ctx-core",
		"google"
	],
	"homepage": "https://github.com/ctx-core/uikit#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/uikit/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/uikit/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rm -rf dist",
		"compile": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"exec": "$@"
	},
	"devDependencies": {
		"@ctx-core/ctx-core-package-tools": "workspace:^8.0.81",
		"@swc/cli": "^0.1.52",
		"@swc/core": "^1.2.118",
		"@types/uikit": "^3.3.2",
		"typescript": "next",
		"uikit": "^3.9.4"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}