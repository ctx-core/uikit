{
	"name": "@ctx-core/uikit",
	"version": "4.2.8",
	"description": "ctx-core uikit",
	"keywords": [
		"ctx-core",
		"google"
	],
	"homepage": "https://github.com/ctx-core/uikit#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/uikit/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/uikit.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"*.scss",
		"./component__register"
	],
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm test-unit && check-dts",
		"test-unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test-unit-coverage": "c8 pnpm test-unit"
	},
	"devDependencies": {
		"@types/uikit": "^3.14.4",
		"c8": "^8.0.1",
		"check-dts": "^0.7.2",
		"tsx": "^4.6.2",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"sideEffects": false
}